{
  "name": "pokemon-gateway",
  "version": "1.0.0",
  "main": "dist/gateway/index.js",
  "scripts": {
    "dev:pokemon": "ts-node src/microservices/pokemon.server.ts",
    "dev:ai": "ts-node src/microservices/ai.server.ts",
    "dev:gateway": "ts-node src/gateway/index.ts",
    "dev": "concurrently \"npm run dev:pokemon\" \"npm run dev:ai\" \"npm run dev:gateway\"",
    "build": "tsc",
    "start:pokemon": "node dist/microservices/pokemon.server.js",
    "start:ai": "node dist/microservices/ai.server.js",
    "start:gateway": "node dist/gateway/index.js",
    "start": "concurrently \"npm run start:pokemon\" \"npm run start:ai\" \"npm run start:gateway\""
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
